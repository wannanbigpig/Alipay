{
    "name": "wannanbigpig/alipay-sdk-php",
    "description": "支付宝接口扩展包",
    "license": "MIT",
    "keywords": [
        "alipay",
        "pay"
    ],
    "type": "library",
    "support": {
        "issues": "https://github.com/wannanbigpig/alipay-sdk-php/issues",
        "source": "https://github.com/wannanbigpig/alipay-sdk-php"
    },
    "authors": [
        {
            "name": "liuml",
            "email": "liumenglei0211@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "wannanbigpig/supports": "^2.1",
        "pimple/pimple": "^3.0",
        "symfony/http-foundation": "^3.0 || ^4.0",
        "ext-json": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.5.*",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "EasyAlipay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EasyAlipay\\Tests\\": "tests"
        }
    }
}
