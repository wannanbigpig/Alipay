{
    "name": "wannanbigpig/alipay",
    "description": "支付宝接口扩展包",
    "license": "MIT",
    "keywords": [
        "alipay",
        "pay"
    ],
    "type": "library",
    "support": {
        "issues": "https://github.com/wannanbigpig/alipay/issues",
        "source": "https://github.com/wannanbigpig/alipay"
    },
    "authors": [
        {
            "name": "liuml",
            "email": "liumenglei0211@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-simplexml":"*",
        "ext-libxml":"*",
        "wannanbigpig/supports": "1.*",
        "ext-iconv": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.5.*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "WannanBigPig\\Alipay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WannanBigPig\\Alipay\\Tests\\": "tests"
        }
    }
}
